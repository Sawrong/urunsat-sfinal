
@{
    ViewData["Title"] = "GetRoleList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model List<AppRole>
<h1>Roller</h1>
<br />
<br />
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>

<a asp-controller="Admin" asp-action="RoleAdd" class="btn btn-primary">Role Ekle</a>